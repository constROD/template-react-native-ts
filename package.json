{
  "name": "TemplateReactNativeTS",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "install:pod": "pnpx pod-install",
    "dev:android": "react-native run-android",
    "dev:ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "format": "prettier --write ./src/**/*.ts{,x}",
    "lint": "eslint ./src --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint ./src --fix --ext .js,.ts,.jsx,.tsx",
    "pre-commit": "lint-staged --allow-empty",
    "prepare": "husky install"
  },
  "dependencies": {
    "@rneui/base": "4.0.0-rc.7",
    "@rneui/themed": "4.0.0-rc.7",
    "axios": "^1.1.3",
    "date-fns": "^2.29.3",
    "immer": "^9.0.16",
    "jsc-android": "^250230.2.1",
    "lodash": "^4.17.21",
    "react": "18.1.0",
    "react-native": "0.70.5",
    "react-native-gradle-plugin": "^0.71.10",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-vector-icons": "^9.2.0",
    "react-router-native": "^6.4.3",
    "zustand": "^4.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@react-native-community/cli-platform-android": "^9.3.1",
    "@react-native-community/cli-platform-ios": "^9.3.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@rnx-kit/metro-config": "^1.3.3",
    "@rnx-kit/metro-resolver-symlinks": "^0.1.23",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.188",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "babel-jest": "^26.6.3",
    "eslint": "8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "6",
    "jest": "^26.6.3",
    "lint-staged": "^13.0.3",
    "metro-react-native-babel-preset": "0.72.3",
    "prettier": "^2.7.1",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": "eslint --cache --fix",
    "**/*": "prettier --write --ignore-unknown"
  }
}
